{
  "name": "crescere",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest run --exclude '**/*.integration.test.js'",
    "test:watch": "vitest --exclude '**/*.integration.test.js'",
    "test:integration": "vitest run --config vitest.integration.config.js",
    "test:all": "vitest run",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write src/",
    "format:check": "prettier --check src/",
    "prepare": "husky",
    "analyze": "node scripts/analyze.js",
    "analyze:open": "node scripts/analyze.js --open",
    "analyze:coverage": "vitest run --exclude '**/*.integration.test.js' --coverage",
    "analyze:bundle": "ANALYZE=true vite build",
    "analyze:duplication": "jscpd src/",
    "analyze:deps": "madge --circular --extensions js src/ && knip",
    "analyze:lighthouse": "node scripts/lighthouse.js",
    "analyze:lighthouse:open": "node scripts/lighthouse.js --open"
  },
  "lint-staged": {
    "*.{js,svelte}": [
      "eslint --fix --no-warn-ignored",
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@tailwindcss/vite": "^4.1.18",
    "@vitest/coverage-v8": "^4.0.18",
    "eslint": "^9.39.2",
    "eslint-plugin-sonarjs": "^3.0.6",
    "eslint-plugin-svelte": "^3.14.0",
    "husky": "^9.1.7",
    "jscpd": "^4.0.8",
    "knip": "^5.83.0",
    "lint-staged": "^16.2.7",
    "madge": "^8.0.0",
    "prettier": "^3.8.1",
    "prettier-plugin-svelte": "^3.4.1",
    "rollup-plugin-visualizer": "^6.0.5",
    "svelte": "^5.43.8",
    "tailwindcss": "^4.1.18",
    "vite": "^7.2.4",
    "vitest": "^4.0.18"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.93.3",
    "chart.js": "^4.5.1",
    "chartjs-plugin-annotation": "^3.1.0",
    "chartjs-plugin-datalabels": "^2.2.0"
  }
}
